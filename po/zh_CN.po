msgid "Failed to remove essential system package"
msgstr "移动系统必需软件包失败"

#: ../aptkit/enums.py:500
msgid "Task cannot be monitored or controlled"
msgstr "无法监视或控制任务"

#: ../aptkit/enums.py:501
msgid "Package operation failed"
msgstr "软件包操作失败"

#: ../aptkit/enums.py:502
msgid "Requires installation of untrusted packages"
msgstr "需要安装不能信任的软件包"

#: ../aptkit/enums.py:504
msgid "Previous installation hasn't been completed"
msgstr "上次安装未完成"

#: ../aptkit/enums.py:505
msgid "The package is of bad quality"
msgstr "该软件包质量欠佳"

#: ../aptkit/enums.py:506
msgid "Package file could not be opened"
msgstr "无法打开软件包文件"

#: ../aptkit/enums.py:507
msgid "Not supported feature"
msgstr "不支持的功能"

#: ../aptkit/enums.py:508
msgid "Failed to download the license key"
msgstr "无法下载许可密钥"

#: ../aptkit/enums.py:509
msgid "Failed to install the license key"
msgstr "无法安装许可密钥"

#: ../aptkit/enums.py:510
msgid "The system is already up to date"
msgstr "该系统已是最新"

#: ../aptkit/enums.py:511
msgid "You could not be authorized"
msgstr "您没有得到授权"

#: ../aptkit/enums.py:512
msgid "You are not allowed to perform this action"
msgstr "您不能执行该操作"

#: ../aptkit/enums.py:513
msgid "An unhandlable error occured"
msgstr "发生了一个无法处理的错误"

#: ../aptkit/enums.py:516
msgid "Waiting for service to start"
msgstr "正在等待服务启动"

#: ../aptkit/enums.py:518
msgid "Waiting"
msgstr "等待中"

#: ../aptkit/enums.py:519
msgid "Waiting for required medium"
msgstr "等待所需的媒体"

#: ../aptkit/enums.py:520
msgid "Waiting for other software managers to quit"
msgstr "等待其他软件管理程序退出"

#: ../aptkit/enums.py:521
msgid "Waiting for configuration file prompt"
msgstr "等待配置文件呼应"

#: ../aptkit/enums.py:523
msgid "Running task"
msgstr "正在运行任务"

#: ../aptkit/enums.py:524
msgid "Downloading"
msgstr "下载中"

#: ../aptkit/enums.py:525
msgid "Querying software sources"
msgstr "正在查询软件源"

#: ../aptkit/enums.py:526
msgid "Cleaning up"
msgstr "清理中"

#: ../aptkit/enums.py:527
msgid "Resolving dependencies"
msgstr "正在解决依赖关系"

#: ../aptkit/enums.py:529
msgid "Finished"
msgstr "完成"

#: ../aptkit/enums.py:530
msgid "Cancelling"
msgstr "正在取消"

#: ../aptkit/enums.py:531
msgid "Loading software list"
msgstr "正在载入软件列表"

#: ../aptkit/enums.py:532
msgid "Waiting for authentication"
msgstr "正在等待验证"

#. TRANSLATORS: %s is the name of a package
#: ../aptkit/enums.py:536
#, python-format
msgid "Installing %s"
